@prefix bdr: <http://example.com/bdr-schema/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix tern: <https://w3id.org/tern/ontologies/tern/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

bdr:SiteShape a sh:NodeShape ;
    sh:property [ sh:datatype xsd:string ;
            sh:path dcterms:identifier ],
        [ sh:hasValue <http://linked.data.gov.au/def/tern-cv/5bf7ae21-a454-440b-bdd7-f2fe982d8de4> ;
            sh:path tern:featureType ],
        [ sh:class tern:SiteVisit ;
            sh:path tern:hasSiteVisit ],
        [ sh:in ( <http://linked.data.gov.au/def/tern-cv/5bf7ae21-a454-440b-bdd7-f2fe982d8de4> <http://linked.data.gov.au/def/tern-cv/ffbe3c8c-23f1-4fc4-8aaf-dfba9f12576c> <http://linked.data.gov.au/def/tern-cv/8cadf069-01d7-4420-b454-cae37740c2a2> <http://linked.data.gov.au/def/tern-cv/4362c8f2-b3cc-4816-b5a2-fb7bb4c0cff5> <http://linked.data.gov.au/def/tern-cv/de46fa49-d1c9-4bef-8462-d7ee5174e1e1> <http://linked.data.gov.au/def/tern-cv/5bf7ae21-a454-440b-bdd7-f2fe982d8de4> ) ;
            sh:path dcterms:type ],
        [ sh:class tern:RDFDataset ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path void:inDataset ] ;
    sh:targetClass tern:Site .

